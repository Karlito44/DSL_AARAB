<?xml version="1.0" encoding="UTF-8"?>
<mydsl:FSM xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:mydsl="http://www.example.org/mydsl"
    xsi:schemaLocation="http://www.example.org/mydsl mydsl.ecore">
  <state xsi:type="mydsl:IntitialState"
      name="Initial"/>
  <state xsi:type="mydsl:FinalState"
      name="Final"/>
  <state name="Open"/>
  <state name="Close"/>
  <transition
      name="t1"
      source="#//@state.0"
      target="#//@state.2"/>
  <transition
      name="t2"
      source="#//@state.2"
      target="#//@state.3"/>
  <transition
      name="t3"
      source="#//@state.3"
      target="#//@state.1"/>
</mydsl:FSM>
